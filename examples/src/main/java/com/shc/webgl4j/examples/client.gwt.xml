<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="examples">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Inherit the WebGL4J library.                         -->
    <inherits name='com.shc.webgl4j.client'/>

    <!-- Inherit the JOML-GWT library.                        -->
    <inherits name='org.joml.JOML'/>

    <!-- Define the entrypoint                                -->
    <entry-point class='com.shc.webgl4j.examples.client.WebGL4J'/>

    <!-- Define the theme                                     -->
    <inherits name='com.google.gwt.user.theme.dark.Dark'/>

    <source path='client'/>

</module>
